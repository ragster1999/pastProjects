module multiplier (m0, m1, m2, m3, q0, q1, q2, q3, p0, p1, p2, p3, p4, p5, p6, p7);
	input m0, m1, m2, m3, q0, q1, q2, q3;
	output p0, p1, p2, p3, p4, p5, p6, p7;
	
endmodule // multiplier

module fullAdder (Cin, x, y, s, Cout);
	input Cin, x, y;
	output s, Cout;
	assign s = x ^ y ^ Cin;
	assign Cout = (X & y) |  (x & Cin) | (y & Cin);
endmodule // fullAdder